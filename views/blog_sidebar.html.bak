<h3>Tags:</h3>
{{for tag in session.blog_tags:}}
    {{=A(tag,_href=URL('view_tag',args=tag),_class="label label-default")}}
{{pass}}

<h3>Users:</h3>
{{for user in session.user_dict:}}
    {{=A(session.user_dict[user],_href=URL('user_posts',vars={'user_id':user}),_class="label label-default")}}
{{pass}}
