<div id="specialties_sidebar">
    <h1>
        Specialties
    </h1>
    <p>
        Choose a specialty
    </p>
</div>
{{links_dict = session.specialty_dict }}
{{if request.args(0):}} 
{{current_link = request.args(0)}}
{{else:}}
{{current_link='test'}}
{{pass}}
<div class="container-fluid">
    <div class="row"><br><br>
      <ul class="nav nav-pills nav-stacked">
          {{for key in sorted(links_dict):}}
              {{if current_link == key:}}
                  {{=LI(A(links_dict[key],_href=URL('view',args=key)),_class="active")}}
              {{else:}}
                  {{=LI(A(links_dict[key],_href=URL('view',args=key)))}}
              {{pass}}
          {{pass}}
      </ul>  
    </div>
    {{=current_link}}
</div>
